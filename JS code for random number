<!DOCTYPE html>
<html lang = "en">
<head>
  <title>Rock, Paper, Scissors</title>
</head>

<body>
  <script>
    var compChoice = (Math.floor(Math.random() * 3) + 1);

    switch (compChoice) {
      case 1:
      document.write("The computer chose rock!")
      break;

      case 2:
      document.write("The computer chose paper!")
      break;

      case 3:
      document.write("The computer chose scissors!")
      break;
    }

    </script>
  </body>
</html>
